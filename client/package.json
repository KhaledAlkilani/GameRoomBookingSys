{
  "name": "gameroombookingsys",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "lint": "eslint .",
    "preview": "vite preview",
    "dev": "vite",
    "build": "tsc -b && vite build && npm run generate-api",
    "generate-api": "npx openapi-typescript-codegen --input http://localhost:5025/swagger/v1/swagger.json --output src/api --client fetch && node generateApiWrapper.cjs",
    "parse-translations": "cd . && node ./tools/move-old-translations.js ./old-translations ./public/locales && \"./node_modules/.bin/i18next\" && node ./tools/postprocess-locales.js ./public/locales && node ./tools/postprocess-default-locales.js ./public/locales/en && node ./tools/move-old-translations.js ./public/locales ./old-translations"
  },
  "proxy": "http://localhost:5025",
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@fullcalendar/daygrid": "^6.1.17",
    "@fullcalendar/interaction": "^6.1.17",
    "@fullcalendar/react": "^6.1.17",
    "@fullcalendar/timegrid": "^6.1.17",
    "@mui/icons-material": "^7.0.1",
    "@mui/material": "^7.0.1",
    "@mui/x-date-pickers": "^7.28.2",
    "dayjs": "^1.11.13",
    "i18next": "^24.2.3",
    "i18next-browser-languagedetector": "^8.0.4",
    "luxon": "^3.6.1",
    "notistack": "^3.0.2",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-i18next": "^15.4.1",
    "react-router-dom": "^6.30.0",
    "stylis-plugin-rtl": "^2.1.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.19.0",
    "@types/luxon": "^3.6.2",
    "@types/react": "^19.0.8",
    "@types/react-dom": "^19.0.3",
    "@types/react-i18next": "^8.1.0",
    "@types/stylis": "^4.2.7",
    "@vitejs/plugin-react": "^4.3.4",
    "eslint": "^9.19.0",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.18",
    "globals": "^15.14.0",
    "i18next-parser": "^9.3.0",
    "openapi-typescript-codegen": "^0.29.0",
    "typescript": "~5.7.2",
    "typescript-eslint": "^8.22.0",
    "vite": "^6.1.0"
  }
}
